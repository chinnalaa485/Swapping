#include<stdio.h>
int main()
{
  int r,c, arr[3][3];
  //Logic to read matrix 
  for(r=0; r<3; r++) // rows
  {
    for(c=0; c<3; c++) //columns
    {
       printf("Enter matrix number[%d][%d]:", r, c);
       scanf("%d", &arr[r][c]);
    }
  }
  printf("The 3 X 3 matrix:\n");
  for(r=0; r<3; r++) // rows
  {
    for(c=0; c<3; c++) //columns
    {
       printf("%d ", arr[r][c]);
    }
    printf("\n");
  }
  return 0;
}
